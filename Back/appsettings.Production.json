{
  "Auth": {
    "Issuer": "syki-api-production",
    "Audience": "syki-api-production",
    "ExpirationTimeInMinutes": 6000
  },
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console" ],
    "MinimumLevel": { "Default": "Warning" },
    "WriteTo": [ { "Name": "Console" } ],
    "Enrich": [ "FromLogContext", "WithMachineName" ]
  }
}
